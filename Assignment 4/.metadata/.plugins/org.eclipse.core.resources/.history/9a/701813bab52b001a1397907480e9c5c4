#include<stdio.h>
int main (){
	int Array[5];
	int sum=0,i;
	int *ptr;

	for (i=0;i<5;i++){
		scanf("%d",Array[i]);
	}
	ptr=Array;
	for(i=0;i<5;i++){
		sum = sum + *ptr;
		ptr++;
	}
	 printf("The sum of array elements : %d", sum);
return 0;
}

