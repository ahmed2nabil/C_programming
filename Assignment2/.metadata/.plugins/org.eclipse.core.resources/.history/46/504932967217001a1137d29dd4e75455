#include<stdio.h>
int calculate(int operand1,int operand2,char operation);
  int main (){

  }
int calculate(int operand1,int operand2,char operation){
	int result;
	switch(operation){
	case'+':
		result=operand1+operand2;
	case'-':
		result=operand1-operand2;
	case'*':
		result=operand1*operand2;
	case'/':
		result=operand1/operand2;
	default :
		result=0;
	}
	return result;
}
